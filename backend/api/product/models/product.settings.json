{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "featured": {
      "type": "boolean",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "products"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "height": {
      "type": "decimal"
    },
    "width": {
      "type": "decimal"
    },
    "description2": {
      "type": "text"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug2": {
      "type": "string"
    },
    "reviews": {
      "via": "product",
      "collection": "review"
    },
    "rating": {
      "type": "decimal",
      "max": 5,
      "min": 1
    },
    "noofreviews": {
      "type": "integer"
    }
  }
}
